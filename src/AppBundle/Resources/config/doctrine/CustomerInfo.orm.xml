<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\CustomerInfo" table="customer_info">
    <indexes>
      <index name="customer_info_customer_id_idx" columns="customer_id"/>
    </indexes>
    <id name="customerInfoId" type="bigint" column="customer_info_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="customerInfoBusinessType" type="integer" column="customer_info_business_type" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="customerInfoIndustry" type="string" column="customer_info_industry" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerInfoHasDomain" type="boolean" column="customer_info_has_domain" nullable="false"/>
    <field name="customerInfoFacebookAdsExpenditure" type="bigint" column="customer_info_facebook_ads_expenditure" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="customerInfoLeadpagesTarget" type="string" column="customer_info_leadpages_target" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customerInfoDomain" type="boolean" column="customer_info_domain" nullable="false"/>
    <field name="createDt" type="datetime" column="create_dt" nullable="true">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="updateDt" type="datetime" column="update_dt" nullable="true">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <many-to-one field="customer" target-entity="Customer" fetch="LAZY">
      <join-columns>
        <join-column name="customer_id" referenced-column-name="customer_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
